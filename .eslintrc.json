{
    "env": {
        "cypress/globals": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:cypress/recommended",
        "plugin:prettier/recommended"
    ],
    "plugins": ["cypress"],
    "rules": {
        "cypress/no-unnecessary-waiting": "off",
        "prettier/prettier": [
            "error",
            {
                "endOfLine": "auto"
            }
        ]
    }
}
